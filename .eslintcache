               <Route path='/games/math' render={ () => {\n                            return(\n                                <>\n                                <MathGame timer={this.state.timer}/>\n                                </>\n                            )}\n                        }/>\n\n                        <Route path=\"/games\" render={ () => {\n                            return (\n                                <>\n                                {this.arrayofGames()}\n                                </>\n                            )}\n                        } />\n\n                    </Switch>\n                    </>\n                : \n                    <Redirect to=\"/home\" />\n                }\n            </>\n        )\n    }\n}\n\nfunction msp(state) {\n    return {user: state.user}\n}\n\n\nexport default connect(msp, null)(GameContainer)","/Users/metrio/Development/freud folds/ff-frontend/src/Components/MathGame.js",[],"/Users/metrio/Development/freud folds/ff-frontend/src/Redux/actions.js",[],"/Users/metrio/Development/freud folds/ff-frontend/src/Redux/actionTypes.js",[],"/Users/metrio/Development/freud folds/ff-frontend/src/Components/GameCard.js",[],"/Users/metrio/Development/freud folds/ff-frontend/src/Components/MathForm.js",[],"/Users/metrio/Development/freud folds/ff-frontend/src/Components/Timer.js",[],"/Users/metrio/Development/freud folds/ff-frontend/src/Components/SignupForm.js",[],"/Users/metrio/Development/freud folds/ff-frontend/src/Components/Navbar.js",[],"/Users/metrio/Development/freud folds/ff-frontend/src/Components/LoginForm.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":60,"column":29,"nodeType":"69","messageId":"70","endLine":60,"endColumn":87},"no-native-reassign",["71"],"no-negated-in-lhs",["72"],"no-lone-blocks","Nested block is redundant.","BlockStatement","redundantNestedBlock","no-global-assign","no-unsafe-negation"]